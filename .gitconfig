[user]
  name = Nick Russo
  email = nijaru0x@gmail.com
  signingkey = 403D399073A3E248
[url "git@github.com:"]
  insteadOf = https://github.com/
[core]
  editor = vim
  excludesfile = /Users/nick/.gitignore_global
  quotepath = false
[color]
  [color "status"]
    changed = yellow
[alias]
  [merge]
    ff = only
    # conflictstyle = diff3
  [commit]
    gpgsign = true
  [push]
    default = simple
    followTags = true
  [status]
    showUntrackedFiles = all
  [transfer]
    fsckobjects = true
  [http]
    cookiefile = /Users/nick/.gitcookies
  [filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
