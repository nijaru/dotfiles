[user]
	name = Nick Russo
	email = nijaru0x@gmail.com
	signingkey = 9C3CF472C6791D53
[checkout]
	defaultRemote = origin
	workers = 0
[color "status"]
	changed = yellow
[commit]
	gpgsign = true
[core]
	editor = code --wait
[fetch]
	parallel = 0
	prune = true
	pruneTags = true
[gpg]
	program = gpg
[init]
	defaultBranch = main
[merge]
	ff = only
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait $MERGED
[pack]
	threads = 0
[pull]
	ff = only
	prune = true
	pruneTags = true
	rebase = true
[push]
	default = upstream
	followTags = true
[rebase]
	autoSquash = true
[rerere]
	autoupdate = true
	enabled = true
[status]
	showUntrackedFiles = all
[submodule]
	fetchJobs = 0
[tag]
	forceSignAnnotated = true
[transfer]
	fsckobjects = true
[url "git@github.com:"]
	insteadOf = https://github.com/
[alias]
	a = add
	b = branch
	c = commit
	cm = commit -sm
	cln = clone
	co = checkout
	d = diff
	f = fetch
	ls = ls-files
	m = merge
	pl = pull
	plr = pull --rebase
	ps = push
	rb = rebase
	rs = restore
	s = stash
	sp = stash pop
	sw = switch
	swc = switch -c
	undo = restore --staged
	w = status
	ws = status --short
