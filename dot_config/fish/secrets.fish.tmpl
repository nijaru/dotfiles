#!/usr/bin/env fish
# API Keys - DO NOT COMMIT TO DOTFILES
# Decrypted from .chezmoidata.yaml.age at apply time

{{- $secrets := include ".chezmoidata.yaml.age" | decrypt | fromYaml }}

# Development API keys
set -gx GITHUB_TOKEN "{{ $secrets.github_token }}"
set -gx CRATES_API_TOKEN "{{ $secrets.crates_api_token }}"
set -gx GEMINI_API_KEY "{{ $secrets.gemini_api_key }}"

# AI API keys
set -gx OPENROUTER_API_KEY "{{ $secrets.openrouter_api_key }}"
set -gx HF_TOKEN "{{ $secrets.hf_token }}"

# MCP/Search API keys
set -gx EXA_API_KEY "{{ $secrets.exa_api_key }}"
set -gx PARALLEL_API_KEY "{{ $secrets.parallel_api_key }}"
