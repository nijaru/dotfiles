#!/usr/bin/env fish
# API Keys - DO NOT COMMIT TO DOTFILES
# Generated from ~/.config/chezmoi/chezmoi.toml

# Development API keys
set -gx GITHUB_TOKEN "{{ .github_token }}"
set -gx CRATES_API_TOKEN "{{ .crates_api_token }}"
set -gx GOOGLE_API_KEY "{{ .google_api_key }}"
set -gx GEMINI_API_KEY $GOOGLE_API_KEY

# AI API keys
set -gx OPENROUTER_API_KEY "{{ .openrouter_api_key }}"

# MCP/Search API keys
set -gx EXA_API_KEY "{{ .exa_api_key }}"
set -gx PARALLEL_API_KEY "{{ .parallel_api_key }}"
set -gx BRAVE_API_KEY "{{ .brave_api_key }}"
set -gx BRAVE_PRO_API_KEY "{{ .brave_pro_api_key }}"
