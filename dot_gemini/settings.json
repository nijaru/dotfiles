{
	"mcpServers": {
		"context7": {
			"command": "npx",
			"args": ["-y", "context7-mcp-server"]
		},
		"exa": {
			"command": "npx",
			"args": ["-y", "exa-mcp-server"],
			"env": {
				"EXA_API_KEY": "$EXA_API_KEY"
			}
		}
	},
	"model": {
		"name": "gemini-3-flash-preview"
	},
	"security": {
		"auth": {
			"selectedType": "oauth-personal"
		}
	},
	"ui": {
		"footer": {
			"hideContextPercentage": false
		},
		"showMemoryUsage": true,
		"theme": "Atom One"
	},
	"context": {
		"fileName": "AGENTS.md"
	},
	"general": {
		"previewFeatures": true
	},
	"tools": {
		"shell": {
			"showColor": true
		}
	}
}
